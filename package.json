{
  "name": "hex-lens",
  "publisher": "szTheory",
  "displayName": "hex-lens",
  "description": "Hover info for Rebar3 (Erlang) and Mix (Elixir) packages on Hex. Shows package version and description.",
  "version": "0.0.2",
  "engines": {
    "vscode": "^1.41.0"
  },
  "galleryBanner": {
    "color": "#5c676d",
    "theme": "dark"
  },
  "categories": [
    "Programming Languages"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/szTheory/vsc-hex-lens"
  },
  "activationEvents": [
    "workspaceContains:mix.exs",
    "workspaceContains:rebar.config"
  ],
  "icon": "images/hex.png",
  "main": "./out/extension.js",
  "bugs": {
    "url": "https://github.com/szTheory/vsc-hex-lens/issues"
  },
  "homepage": "https://github.com/szTheory/vsc-hex-lens",
  "contributes": {
    "languages": [
      {
        "id": "erlang",
        "aliases": [
          "Erlang",
          "erlang"
        ],
        "filenames": [
          "rebar.config"
        ]
      },
      {
        "id": "elixir",
        "aliases": [
          "Elixir",
          "elixir"
        ],
        "filenames": [
          "mix.exs"
        ]
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.41.0",
    "glob": "^7.1.5",
    "mocha": "^10.2.0",
    "tslint": "^5.20.0",
    "typescript": "^3.6.4",
    "vscode-test": "^1.2.2"
  },
  "dependencies": {
    "axios": "^0.19.1"
  }
}
